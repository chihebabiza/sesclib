<%- include('../parts/head') %>
    <%- include('../parts/nav') %>

        <section id="resources" class="white-section">
            <div class="container-fluid">
                <div class="row">
                    <% majors.forEach(major=> { %>
                        <div class="col-lg-3 col-md-6">
                            <div class="specialty-card">
                                <a href="/major/<%= major._id %>">
                                    <img src="<%= major.image %>" alt="<%= major.name %>" class="specialty-image">
                                </a>
                                <a class="btn btn-primary specialty-button">
                                    <%= major.name %>
                                </a>
                            </div>
                        </div>
                        <% }); %>
                </div>
            </div>
        </section>